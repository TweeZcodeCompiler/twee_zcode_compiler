body{
	margin:0 auto;	
}

.body.content{
	overflow-y:visible;
}

#capecamdenmain{
	width:100%;
	max-width:100%;
	height:100vh;
	border:none;
	margin:0 auto;
	position:relative;
	text-align:center;
}

#starrysky{
	width:100%;
	height:4500px;
	background:#FFFFFF;
	border:none;
	margin:0 auto;
	color:#FFFFFF;
	text-align:center;
	overflow-x:hidden;
	overflow-y:visible;
	z-index:1;
}

#starryskybkgd {
	width:100%;
	height:4500px;
	margin-left:auto;
	margin-right:auto;
	overflow:hidden;
	position:absolute;
}

.gradientback{
    position:absolute;
    top:0px;
    left:0px;
	overflow-x:hidden;
	overflow-y:visible;
    width:100%;
	z-index:2;
    height:4500px;
 background: -moz-linear-gradient(top,  rgba(0,0,0,1) 40%, rgba(0,0,0,1) 41%, rgba(0,0,0,1) 45%, rgba(32,32,32,1) 52%, rgba(41,41,41,1) 54%, rgba(64,64,64,1) 59%, rgba(241,241,241,0.08) 97%, rgba(255,255,255,0.08) 100%); /* FF3.6+ */
background: -webkit-gradient(linear, left top, left bottom, color-stop(40%,rgba(0,0,0,1)), color-stop(41%,rgba(0,0,0,1)), color-stop(45%,rgba(0,0,0,1)), color-stop(52%,rgba(32,32,32,1)), color-stop(54%,rgba(41,41,41,1)), color-stop(59%,rgba(64,64,64,1)), color-stop(97%,rgba(241,241,241,0.08)), color-stop(100%,rgba(255,255,255,0.08))); /* Chrome,Safari4+ */
background: -webkit-linear-gradient(top,  rgba(0,0,0,1) 40%,rgba(0,0,0,1) 41%,rgba(0,0,0,1) 45%,rgba(32,32,32,1) 52%,rgba(41,41,41,1) 54%,rgba(64,64,64,1) 59%,rgba(241,241,241,0.08) 97%,rgba(255,255,255,0.08) 100%); /* Chrome10+,Safari5.1+ */
background: -o-linear-gradient(top,  rgba(0,0,0,1) 40%,rgba(0,0,0,1) 41%,rgba(0,0,0,1) 45%,rgba(32,32,32,1) 52%,rgba(41,41,41,1) 54%,rgba(64,64,64,1) 59%,rgba(241,241,241,0.08) 97%,rgba(255,255,255,0.08) 100%); /* Opera 11.10+ */
background: -ms-linear-gradient(top,  rgba(0,0,0,1) 40%,rgba(0,0,0,1) 41%,rgba(0,0,0,1) 45%,rgba(32,32,32,1) 52%,rgba(41,41,41,1) 54%,rgba(64,64,64,1) 59%,rgba(241,241,241,0.08) 97%,rgba(255,255,255,0.08) 100%); /* IE10+ */
background: linear-gradient(to bottom,  rgba(0,0,0,1) 40%,rgba(0,0,0,1) 41%,rgba(0,0,0,1) 45%,rgba(32,32,32,1) 52%,rgba(41,41,41,1) 54%,rgba(64,64,64,1) 59%,rgba(241,241,241,0.08) 97%,rgba(255,255,255,0.08) 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#000000', endColorstr='#14ffffff',GradientType=0 ); /* IE6-9 */


}

